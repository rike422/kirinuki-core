{
  "name": "kirinuki-core",
  "description": "Parse html by CSS query selectors",
  "version": "2.1.1",
  "author": "Akira Nitta",
  "ava": {
    "files": [
      "src/**/*_test.js"
    ],
    "require": [
      "babel-register",
      "./test_helper"
    ],
    "verbose": true,
    "noCache": true
  },
  "bugs": {
    "url": "https://github.com/rike422/kirinuki-core/issues"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-power-assert": "2.0.0",
    "codecov": "3.1.0",
    "flow": "0.2.3",
    "flow-bin": "0.89.0",
    "jsdom": "13.1.0",
    "nyc": "13.1.0",
    "power-assert": "1.6.1",
    "prettier": "1.15.3",
    "release-it": "9.6.4",
    "rollup": "0.68.2",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-uglify": "5.0.2",
    "sinon": "7.2.2",
    "snazzy": "8.0.0"
  },
  "files": [
    "lib/**/*.js"
  ],
  "homepage": "https://github.com/rike422/kirinuki-core#readme",
  "keywords": [
    "js"
  ],
  "license": "MIT",
  "main": "lib/kirinuki.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rike422/kirinuki-core.git"
  },
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "babel src -d lib --ignore *_test.js",
    "build:browser": "rollup -c",
    "clean": "rm -rf lib/*",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "NODE_ENV=AVA ava",
    "test:ci": "NODE_ENV=AVA BABEL_DISABLE_CACHE=1 nyc ava",
    "test:watch": "NODE_ENV=AVA ava --watch"
  }
}
