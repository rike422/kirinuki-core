{
  "name": "kirinuki-core",
  "description": "Parse html by CSS query selectors",
  "version": "3.1.0",
  "author": "Akira Nitta",
  "ava": {
    "files": [
      "src/**/*_test.ts"
    ],
    "compileEnhancements": false,
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/rike422/kirinuki-core/issues"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.30",
    "@types/jsdom": "12.2.4",
    "@types/node": "11.15.54",
    "@types/pluralize": "0.0.29",
    "@types/power-assert": "1.5.4",
    "ava": "2.4.0",
    "codecov": "3.7.0",
    "jsdom": "15.2.1",
    "nyc": "14.1.1",
    "power-assert": "1.6.1",
    "prettier": "1.19.1",
    "release-it": "14.11.4",
    "rollup": "1.32.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-typescript": "1.0.1",
    "rollup-plugin-uglify": "6.0.4",
    "sinon": "7.5.0",
    "snazzy": "8.0.0",
    "ts-node": "9.1.1",
    "ts-node-test-register": "9.0.1",
    "tslib": "2.3.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.2.0",
    "typescript": "4.3.5"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "README.md",
    "package.json"
  ],
  "homepage": "https://github.com/rike422/kirinuki-core#readme",
  "keywords": [
    "js"
  ],
  "license": "MIT",
  "main": "lib/kirinuki.js",
  "type": "lib/kirinuki.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rike422/kirinuki-core.git"
  },
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "tsc -b",
    "build:browser": "rollup -c",
    "clean": "rm -rf lib/*",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "tsc && NODE_ENV=AVA ava",
    "test:ci": "tsc && NODE_ENV=AVA nyc --extension .ts ava",
    "test:watch": "NODE_ENV=AVA tsc && ava --watch"
  }
}
