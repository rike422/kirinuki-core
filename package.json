{
  "name": "kirinuki-core",
  "description": "Parse html by CSS query selectors",
  "version": "3.1.0",
  "author": "Akira Nitta",
  "ava": {
    "files": [
      "src/**/*_test.ts"
    ],
    "compileEnhancements": false,
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/rike422/kirinuki-core/issues"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.11",
    "@types/jsdom": "12.2.3",
    "@types/node": "11.12.0",
    "@types/pluralize": "0.0.29",
    "@types/power-assert": "1.5.0",
    "ava": "1.4.1",
    "codecov": "3.2.0",
    "jsdom": "13.2.0",
    "nyc": "13.3.0",
    "power-assert": "1.6.1",
    "prettier": "1.16.4",
    "release-it": "10.3.1",
    "rollup": "1.7.4",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.2",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-typescript": "1.0.1",
    "rollup-plugin-uglify": "6.0.2",
    "sinon": "7.3.1",
    "snazzy": "8.0.0",
    "ts-node": "8.0.3",
    "ts-node-test-register": "8.0.0",
    "tslib": "1.9.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.4000"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "README.md",
    "package.json"
  ],
  "homepage": "https://github.com/rike422/kirinuki-core#readme",
  "keywords": [
    "js"
  ],
  "license": "MIT",
  "main": "lib/kirinuki.js",
  "type": "lib/kirinuki.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rike422/kirinuki-core.git"
  },
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "tsc -b",
    "build:browser": "rollup -c",
    "clean": "rm -rf lib/*",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "tsc && NODE_ENV=AVA ava",
    "test:ci": "tsc && NODE_ENV=AVA nyc --extension .ts ava",
    "test:watch": "NODE_ENV=AVA tsc && ava --watch"
  }
}
